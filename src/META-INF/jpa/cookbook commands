query-peopleinsert-person {"email": "sascha.baumeister@gmail.com", "group": "ADMIN", "name": {"family": "Baumeister", "given": "Sascha"}, "address": {"street": "Torstraße 1", "city": "Berlin", "country": "Berlin", "postcode": "10500"}}